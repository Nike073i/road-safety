{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": { 
    "Ð¡onnectionString": "User ID=postgres;Password=postgres;Host=localhost;Port=5445;Database=road_safety"
  },
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.RollingFile" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { 
        "Name": "Console",
        "Args": { 
            "outputTemplate":
              "[{Timestamp:HH:mm:ss}] [{Level:u3}] [{EventId}] - {Message} {Exception} {Errors} {NewLine}"
        } 
      },
      { 
        "Name": "RollingFile", 
        "Args": { 
            "pathFormat": "Logs/log-{Date}.log",
            "outputTemplate":
              "[{Timestamp:HH:mm:ss}] [{Level:u3}] [{EventId}] - {Message} {Exception} {Errors} {NewLine}"
        } 
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Authentication": {
    "Audience": "road-safety-ui-client",
    "Issuer": "http://localhost:8080/realms/road-safety",
    "MetadataUrl": "http://localhost:8080/realms/road-safety/.well-known/openid-configuration",
    "RequireHttpsMetadata": false
  }
}
